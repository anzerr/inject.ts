	"devDependencies": {
		"@types/node": "^11.13.4",
		"eslint": "^6.8.0",
		"eslint-config-typescript": "git+https://git@github.com/anzerr/eslint-config-typescript.git",
		"ts-node": "^8.1.0",
		"typescript": "^3.4.3"
	},
	"engines": {
		"node": ">= 0.10.0"
	}
