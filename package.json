{
	"name": "inject.ts",
	"version": "1.0.18",
	"description": "Dependency injection decorator",
	"keywords": [
		"injection",
		"nodejs",
		"typescipt",
		"util"
	],
	"homepage": "https://github.com/anzerr/inject.ts#readme",
	"bugs": {
		"url": "https://github.com/anzerr/inject.ts/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:anzerr/inject.ts.git"
	},
	"license": "MIT",
	"author": "anzerr",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
